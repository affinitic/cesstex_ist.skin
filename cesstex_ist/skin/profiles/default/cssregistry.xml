    <stylesheet title="style pour tinyMCE"
                id="editeur_interne_style.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                expression=""/>

    <stylesheet title="style pour les portlets"
                id="portlet.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                expression=""/>

