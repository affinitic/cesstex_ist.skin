<?xml version="1.0"?>
<object name="portal_css">

    <stylesheet title="CESSTEX Main Style"
                id="cesstex.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                expression=""/>

    <stylesheet title="IST Style"
                id="ist.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                expression=""/>

    <stylesheet title="jquery"
                id="jquery-ui.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                expression=""/>

    <stylesheet title="HOME-PAGE-FLASH Style"
                id="home_page.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                expression="object/isHomePage"/>

    <stylesheet title="style pour les formulaires"
                id="formulaire_style.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
