                id="++theme++cesstex_ist.skin/font.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>

    <stylesheet title="IST HomePage Styles"
                id="++theme++cesstex_ist.skin/ist/css/style.css"
                media="screen"
